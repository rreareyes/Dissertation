@article{mousavi_gigerenzer_2017_heuristics_are,
  title = {Heuristics Are {{Tools}} for {{Uncertainty}}},
  author = {Mousavi, Shabnam and Gigerenzer, Gerd},
  year = {2017},
  month = dec,
  journal = {Homo Oeconomicus},

}

@article{oh-descher_etal_2017_probabilistic_inference,
  title = {Probabilistic Inference under Time Pressure Leads to a Cortical-to-Subcortical Shift in Decision Evidence Integration},
  author = {{Oh-Descher}, Hanna and Beck, Jeffrey M. and Ferrari, Silvia and Sommer, Marc A. and Egner, Tobias},
  year = {2017},

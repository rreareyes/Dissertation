@article{wei_etal_2016_restingstate_functional,
  title = {Resting-State Functional Connectivity between the Dorsal Anterior Cingulate Cortex and Thalamus Is Associated with Risky Decision-Making in Nicotine Addicts},
  author = {Wei, Zhengde and Yang, Nannan and Liu, Ying and Yang, Lizhuang and Wang, Ying and Han, Long and Zha, Rujing and Huang, Ruiqi and Zhang, Peng and Zhou, Yifeng and Zhang, Xiaochu},
